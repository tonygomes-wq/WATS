# Proteger arquivos do diretório storage
# Negar acesso direto a todos os arquivos

<IfModule mod_authz_core.c>
    Require all denied
</IfModule>

<IfModule !mod_authz_core.c>
    Order deny,allow
    Deny from all
</IfModule>

